bind generic + !git commit --amend # Allow to amend the last commit
bind generic 9 !@sh -c "echo -n %(commit) | xclip -selection c" # Copy commit ID to clipboard
bind refs 3 !git rebase -i %(branch) # Rebase current branch on top of the selected branch

